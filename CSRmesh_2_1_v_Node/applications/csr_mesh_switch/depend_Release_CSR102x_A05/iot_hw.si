 104:../mesh_common/mesh/drivers/iot_hw.c ****  *
 105:../mesh_common/mesh/drivers/iot_hw.c ****  *  DESCRIPTION
 106:../mesh_common/mesh/drivers/iot_hw.c ****  *      This function initialises the LED.
 107:../mesh_common/mesh/drivers/iot_hw.c ****  *      Configure the IO lines connected to Switch as inputs.
 108:../mesh_common/mesh/drivers/iot_hw.c ****  *
 109:../mesh_common/mesh/drivers/iot_hw.c ****  *  RETURNS
 110:../mesh_common/mesh/drivers/iot_hw.c ****  *      Nothing.
 111:../mesh_common/mesh/drivers/iot_hw.c ****  *
 112:../mesh_common/mesh/drivers/iot_hw.c ****  *---------------------------------------------------------------------------*/
 113:../mesh_common/mesh/drivers/iot_hw.c **** extern void IOTLightControlDeviceInit(void)
 114:../mesh_common/mesh/drivers/iot_hw.c **** {
 115:../mesh_common/mesh/drivers/iot_hw.c ****     /* Configure the LED_PIO_RED PIO as output PIO */
 116:../mesh_common/mesh/drivers/iot_hw.c ****     PioSetDir(LED_PIO_RED, PIO_DIRECTION_OUTPUT);
 117:../mesh_common/mesh/drivers/iot_hw.c **** 
 118:../mesh_common/mesh/drivers/iot_hw.c ****     /* Configure the LED_PIO_GREEN PIO as output PIO */
 119:../mesh_common/mesh/drivers/iot_hw.c ****     PioSetDir(LED_PIO_GREEN, PIO_DIRECTION_OUTPUT);
 120:../mesh_common/mesh/drivers/iot_hw.c **** 
 121:../mesh_common/mesh/drivers/iot_hw.c ****     /* Configure the LED_PIO_BLUE PIO as output PIO */
 122:../mesh_common/mesh/drivers/iot_hw.c ****     PioSetDir(LED_PIO_BLUE, PIO_DIRECTION_OUTPUT);
 123:../mesh_common/mesh/drivers/iot_hw.c **** 
 124:../mesh_common/mesh/drivers/iot_hw.c ****     /* Open a PWM instance */
 125:../mesh_common/mesh/drivers/iot_hw.c ****     pwm_ramp_red = PioOpenPWM(LED_PIO_RED, pio_pwm_ramp);
 126:../mesh_common/mesh/drivers/iot_hw.c ****     pwm_ramp_green = PioOpenPWM(LED_PIO_GREEN, pio_pwm_ramp);
 127:../mesh_common/mesh/drivers/iot_hw.c ****     pwm_ramp_blue = PioOpenPWM(LED_PIO_BLUE, pio_pwm_ramp);
 128:../mesh_common/mesh/drivers/iot_hw.c ****     pio_open = TRUE;
 129:../mesh_common/mesh/drivers/iot_hw.c **** 
 130:../mesh_common/mesh/drivers/iot_hw.c ****     /* Set the light to OFF state on initialization */
 131:../mesh_common/mesh/drivers/iot_hw.c ****     IOTLightControlDevicePower(FALSE);
 132:../mesh_common/mesh/drivers/iot_hw.c **** }
 133:../mesh_common/mesh/drivers/iot_hw.c **** 
 134:../mesh_common/mesh/drivers/iot_hw.c **** /*----------------------------------------------------------------------------*
 135:../mesh_common/mesh/drivers/iot_hw.c ****  *  NAME
 136:../mesh_common/mesh/drivers/iot_hw.c ****  *      IOTLightControlDevicePower
 137:../mesh_common/mesh/drivers/iot_hw.c ****  *
 138:../mesh_common/mesh/drivers/iot_hw.c ****  *  DESCRIPTION
 139:../mesh_common/mesh/drivers/iot_hw.c ****  *      This function sets power state of LED.
 140:../mesh_common/mesh/drivers/iot_hw.c ****  *
 141:../mesh_common/mesh/drivers/iot_hw.c ****  *  RETURNS
 142:../mesh_common/mesh/drivers/iot_hw.c ****  *      Nothing.
 143:../mesh_common/mesh/drivers/iot_hw.c ****  *
 144:../mesh_common/mesh/drivers/iot_hw.c ****  *---------------------------------------------------------------------------*/
 145:../mesh_common/mesh/drivers/iot_hw.c **** extern void IOTLightControlDevicePower(bool power_on)
 146:../mesh_common/mesh/drivers/iot_hw.c **** {
 147:../mesh_common/mesh/drivers/iot_hw.c ****     if (power_on == TRUE)
 148:../mesh_common/mesh/drivers/iot_hw.c ****     {
 149:../mesh_common/mesh/drivers/iot_hw.c ****         if(pio_open == FALSE)
 150:../mesh_common/mesh/drivers/iot_hw.c ****         {
 151:../mesh_common/mesh/drivers/iot_hw.c ****             /* Open the pwm on switch on */
 152:../mesh_common/mesh/drivers/iot_hw.c ****             pwm_ramp_red = PioOpenPWM(LED_PIO_RED, pio_pwm_ramp);
 153:../mesh_common/mesh/drivers/iot_hw.c ****             pwm_ramp_green = PioOpenPWM(LED_PIO_GREEN, pio_pwm_ramp);
 154:../mesh_common/mesh/drivers/iot_hw.c ****             pwm_ramp_blue = PioOpenPWM(LED_PIO_BLUE, pio_pwm_ramp);
 155:../mesh_common/mesh/drivers/iot_hw.c ****             pio_open = TRUE;
 156:../mesh_common/mesh/drivers/iot_hw.c ****         }
 157:../mesh_common/mesh/drivers/iot_hw.c ****         /* Enable PWM channel */
 158:../mesh_common/mesh/drivers/iot_hw.c ****         PioEnablePWM(pwm_ramp_red, TRUE);
 159:../mesh_common/mesh/drivers/iot_hw.c ****         PioEnablePWM(pwm_ramp_green, TRUE);
 160:../mesh_common/mesh/drivers/iot_hw.c ****         PioEnablePWM(pwm_ramp_blue, TRUE);
 161:../mesh_common/mesh/drivers/iot_hw.c ****     }
 162:../mesh_common/mesh/drivers/iot_hw.c ****     else if(pio_open == TRUE)
 163:../mesh_common/mesh/drivers/iot_hw.c ****     {
 164:../mesh_common/mesh/drivers/iot_hw.c ****         /* When power off is selected, disable and close all PWMs */
 165:../mesh_common/mesh/drivers/iot_hw.c ****         PioEnablePWM(pwm_ramp_red, FALSE);
 166:../mesh_common/mesh/drivers/iot_hw.c ****         PioEnablePWM(pwm_ramp_green, FALSE);
 167:../mesh_common/mesh/drivers/iot_hw.c ****         PioEnablePWM(pwm_ramp_blue, FALSE);
 168:../mesh_common/mesh/drivers/iot_hw.c **** 
 169:../mesh_common/mesh/drivers/iot_hw.c ****         PioClosePWM(pwm_ramp_red);
 170:../mesh_common/mesh/drivers/iot_hw.c ****         PioClosePWM(pwm_ramp_green);
 171:../mesh_common/mesh/drivers/iot_hw.c ****         PioClosePWM(pwm_ramp_blue);
 172:../mesh_common/mesh/drivers/iot_hw.c **** 
 173:../mesh_common/mesh/drivers/iot_hw.c ****         PioSet(LED_PIO_RED, 1);
 174:../mesh_common/mesh/drivers/iot_hw.c ****         PioSet(LED_PIO_GREEN, 1);
 175:../mesh_common/mesh/drivers/iot_hw.c ****         PioSet(LED_PIO_BLUE, 1);
 176:../mesh_common/mesh/drivers/iot_hw.c ****         pio_open = FALSE;
 177:../mesh_common/mesh/drivers/iot_hw.c ****     }
 178:../mesh_common/mesh/drivers/iot_hw.c **** }
