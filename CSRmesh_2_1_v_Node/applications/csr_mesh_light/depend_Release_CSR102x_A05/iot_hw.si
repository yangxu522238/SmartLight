 141:../mesh_common/mesh/drivers/iot_hw.c ****  *  NAME
 142:../mesh_common/mesh/drivers/iot_hw.c ****  *      IOTLightControlDevicePower
 143:../mesh_common/mesh/drivers/iot_hw.c ****  *
 144:../mesh_common/mesh/drivers/iot_hw.c ****  *  DESCRIPTION
 145:../mesh_common/mesh/drivers/iot_hw.c ****  *      This function sets power state of LED.
 146:../mesh_common/mesh/drivers/iot_hw.c ****  *
 147:../mesh_common/mesh/drivers/iot_hw.c ****  *  RETURNS
 148:../mesh_common/mesh/drivers/iot_hw.c ****  *      Nothing.
 149:../mesh_common/mesh/drivers/iot_hw.c ****  *
 150:../mesh_common/mesh/drivers/iot_hw.c ****  *---------------------------------------------------------------------------*/
 151:../mesh_common/mesh/drivers/iot_hw.c **** extern void IOTLightControlDevicePower(bool power_on)
 152:../mesh_common/mesh/drivers/iot_hw.c **** {
 153:../mesh_common/mesh/drivers/iot_hw.c ****     if (power_on == TRUE)
 154:../mesh_common/mesh/drivers/iot_hw.c ****     {
 155:../mesh_common/mesh/drivers/iot_hw.c ****         if(pio_open == FALSE)
 156:../mesh_common/mesh/drivers/iot_hw.c ****         {
 157:../mesh_common/mesh/drivers/iot_hw.c ****             /* Open the pwm on switch on */
 158:../mesh_common/mesh/drivers/iot_hw.c ****             pwm_ramp_red = PioOpenPWM(LED_PIO_RED, pio_pwm_ramp);
 159:../mesh_common/mesh/drivers/iot_hw.c ****             pwm_ramp_green = PioOpenPWM(LED_PIO_GREEN, pio_pwm_ramp);
 160:../mesh_common/mesh/drivers/iot_hw.c ****             pwm_ramp_blue = PioOpenPWM(LED_PIO_BLUE, pio_pwm_ramp);
 161:../mesh_common/mesh/drivers/iot_hw.c ****             pio_open = TRUE;
 162:../mesh_common/mesh/drivers/iot_hw.c ****         }
 163:../mesh_common/mesh/drivers/iot_hw.c ****         /* Enable PWM channel */
 164:../mesh_common/mesh/drivers/iot_hw.c ****         PioEnablePWM(pwm_ramp_red, TRUE);
 165:../mesh_common/mesh/drivers/iot_hw.c ****         PioEnablePWM(pwm_ramp_green, TRUE);
 166:../mesh_common/mesh/drivers/iot_hw.c ****         PioEnablePWM(pwm_ramp_blue, TRUE);
 167:../mesh_common/mesh/drivers/iot_hw.c ****     }
 168:../mesh_common/mesh/drivers/iot_hw.c ****     else if(pio_open == TRUE)
 169:../mesh_common/mesh/drivers/iot_hw.c ****     {
 170:../mesh_common/mesh/drivers/iot_hw.c ****         /* When power off is selected, disable and close all PWMs */
 171:../mesh_common/mesh/drivers/iot_hw.c ****         PioEnablePWM(pwm_ramp_red, FALSE);
 172:../mesh_common/mesh/drivers/iot_hw.c ****         PioEnablePWM(pwm_ramp_green, FALSE);
 173:../mesh_common/mesh/drivers/iot_hw.c ****         PioEnablePWM(pwm_ramp_blue, FALSE);
 174:../mesh_common/mesh/drivers/iot_hw.c **** 
 175:../mesh_common/mesh/drivers/iot_hw.c ****         PioClosePWM(pwm_ramp_red);
 176:../mesh_common/mesh/drivers/iot_hw.c ****         PioClosePWM(pwm_ramp_green);
 177:../mesh_common/mesh/drivers/iot_hw.c ****         PioClosePWM(pwm_ramp_blue);
 178:../mesh_common/mesh/drivers/iot_hw.c **** 
 179:../mesh_common/mesh/drivers/iot_hw.c ****         PioSet(LED_PIO_RED, 1);
 180:../mesh_common/mesh/drivers/iot_hw.c ****         PioSet(LED_PIO_GREEN, 1);
 181:../mesh_common/mesh/drivers/iot_hw.c ****         PioSet(LED_PIO_BLUE, 1);
 182:../mesh_common/mesh/drivers/iot_hw.c ****         pio_open = FALSE;
 183:../mesh_common/mesh/drivers/iot_hw.c ****     }
 184:../mesh_common/mesh/drivers/iot_hw.c **** }
